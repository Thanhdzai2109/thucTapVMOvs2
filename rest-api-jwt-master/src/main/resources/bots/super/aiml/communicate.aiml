<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<category>
    <pattern>*</pattern>
    <template>Tôi không hiểu bạn vui lòng chat help</template>
</category>
<!-- Hỏi tên -->
<category>
  <pattern>TÊN GÌ</pattern>
  <template>Tôi là một chương trình trí tuệ nhân tạo không có tên. Bạn có thể gọi tôi là Super.</template>
</category>

<category>
    <pattern>Tôi tên là *</pattern>
    <template>Chào <set name = "username"><star index="1"></star></set> , rất vui được gặp bạn</template>
</category>

<category>
    <pattern>tạm biệt</pattern>
    <template>Chào bạn <get name="username"></get></template>
</category>

<category>
    <pattern>^ chào ^</pattern>
    <template>
        <random>
            <li>Chào bạn</li>
            <li>Rất vui được gặp bạn</li>
            <li>Tôi có thể giúp gì cho bạn</li>
        </random>
    </template>
</category>


<category>
    <pattern>help</pattern>
    <template>
        <think><set name="topic">unknown</set></think>
        <button >
            <text>Tính chỉ số BMI</text>
            <postback>Tính chỉ số BMI</postback>
        </button>
        <button>
            <text>Tính lượng kalo tiêu thụ</text>
            <postback>Tính lượng kalo tiêu thụ</postback>
        </button>
        <button>
            <text>Tư vấn tăng cân</text>
            <postback>Tư vấn tăng cân</postback>
        </button>
        <button>
            <text>Chế độ ăn theo thể trạng 3-5 tuổi</text>
            <postback>Chế độ ăn theo thể trạng 3-5 tuổi</postback>
        </button>
        <button>
            <text>Chế độ ăn theo hiện tượng 3-5 tuổi</text>
            <postback>Chế độ ăn theo hiện tượng 3-5 tuổi</postback>
        </button>
    </template>
</category>
</aiml>
